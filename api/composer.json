{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "knplabs/knp-paginator-bundle": "^6.0",
        "league/flysystem": "^3.12",
        "league/flysystem-bundle": "^3.1",
        "league/flysystem-ftp": "^3.10",
        "nelmio/api-doc-bundle": "^4.11",
        "nelmio/cors-bundle": "^2.2",
        "ramsey/uuid": "^4.6",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": "^6.1",
        "symfony/dotenv": "6.1.*",
        "symfony/filesystem": "6.1.*",
        "symfony/flex": "^2",
        "symfony/form": "6.1.*",
        "symfony/framework-bundle": "6.1.*",
        "symfony/mailer": "6.1.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/security-bundle": "6.1.*",
        "symfony/twig-bundle": "6.1.*",
        "symfony/validator": "6.1.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "6.1.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "lint": "phplint",
        "php-cs-fixer": "php-cs-fixer --config=.php-cs-fixer.php --allow-risky=yes",
        "psalm": "psalm --config=psalm.xml",
        "unit-test": "phpunit --colors=always",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/test/coverage",
        "app": "php bin/app.php --ansi"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.13",
        "fzaninotto/faker": "^1.5",
        "lctrs/psalm-psr-container-plugin": "^1.8",
        "overtrue/phplint": "^5.3",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.18.3",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "6.1.*",
        "symfony/css-selector": "6.1.*",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "6.1.*",
        "symfony/web-profiler-bundle": "6.1.*",
        "vimeo/psalm": "^4.30"
    }
}
